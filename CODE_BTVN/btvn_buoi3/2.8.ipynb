    "import json\n",
    "\n",
    "# Đối tượng Python (dict)\n",
    "person = {\n",
    "    \"name\": \"Zhang Ruonan\",\n",
    "    \"age\": 25,\n",
    "    \"city\": \"Beijing\"\n",
    "}\n",
    "\n",
    "# Chuyển sang JSON\n",
    "json_data = json.dumps(person, ensure_ascii=False, indent=4)\n",
    "\n",
    "print(\"Đối tượng Python:\", person)\n",
    "print(\"Chuỗi JSON:\\n\", json_data)"
   ]
  }
