   "source": [
    "from xml.dom import minidom\n",
    "\n",
    "# Đọc file XML\n",
    "doc = minidom.parse(\"sample.xml\")\n",
    "\n",
    "# Lấy danh sách thẻ <name>\n",
    "names = doc.getElementsByTagName(\"name\")\n",
    "\n",
    "print(\"Danh sách nhân viên:\")\n",
    "for name in names:\n",
    "    print(\"-\", name.firstChild.data)"
   ]
  }
